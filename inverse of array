
public class Main
{   public static void reverse(int arr[],int res[]){
        int temp;
        for(int i=0;i<arr.length;i++){
            temp=arr[i];
            res[temp]=i;
        }
    
    
    
}
	public static void main(String[] args) {
		System.out.println("Hello World");
		int arr[]=new int[]{2,5,0,3,4,1};
		int res[] =new int[6];
		reverse(arr,res);
		for(int ele:res)
		    System.out.print(ele+" ");
	}
}
